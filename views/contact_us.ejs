<%- include("partials/headerforall")%>

<h3 id="contact_us_head">
	Contact Us!
</h3>
  <form id="contact_us_form" action="/contact" method="POST">
    <input class="contact_us_inputs" type="text" name="name" placeholder="Fullname">
	<p>
		<input class="contact_us_inputs" type="email" name="email" placeholder="abc@xyz.com">
	</p>
	<p>
		<input class="contact_us_inputs" type="text" name="subject" placeholder="Subject">
	</p>	  
	<p>
		<textarea class="contact_us_inputs" id="contact_us_query" name="query" placeholder="Write something.."></textarea> 
	</p>
	  
    <button id="contact_us_submit_button">
		Submit!
	</button>	  
  </form>

<div id="contact_us_trademarkset">
	<p id="tradeMark">
		Trademark NewNormal 2020. All Rights Reserved
	</p>
	<!-- <p id="createdBy">
		BY KD
	</p> -->
</div>
<script src="/js/fade.ejs"></script>
</body>
</html>