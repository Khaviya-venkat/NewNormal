<%- include("partials/headerforall")%>

<div>
	<h3 id="signup_head">
		SignUp
	</h3>
	  <form id="signup_form" action="/signup" method="POST">
	  	<input class="signup_inputs" type="text" name="username" placeholder="Username">
		<br>
			<input class="signup_inputs" type="password" name="password" placeholder="Password">
		<br>
		
			<input class="signup_inputs" type="email" name="email" placeholder="E-mail">	
		

		  <p id="radiobuttons">
			 <input type="radio" class="radio" id="user" name="loginas" value="user">
			  <label for="user">User</label><br>
			  <input type="radio" class="radio" id="doctor" name="loginas" value="doctor">
			  <label for="doctor">Doctor</label><br> 
			  <input type="radio" class="radio" id="companyhr" name="loginas" value="companyhr">
			  <label for="companyhr">Company HR</label><br> 
			  <input type="radio" class="radio" id="teacher" name="loginas" value="teacher">
			  <label for="teacher">Teacher</label><br>
		  </p>

		<button id="signup_button">
			SignUp!
		</button>	  

	  </form>

	<p id="signin_line">
		Already have an account? <a href="/" id="signup"> Login!</a>
	</p>
</div>

<div id="signup_trademarkset">
	<p id="tradeMark">
